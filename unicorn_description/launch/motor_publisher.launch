	
    
<?xml version="1.0" encoding="utf-8"?>
<launch>
    
    
      <!-- Load joint controller configurations from YAML file to parameter server -->
	<rosparam file="$(find unicorn_description)/config/robot_control.yaml" command="load"/>

	<!-- load the controllers -->
	<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
		output="screen" ns="/" args="lift_vertical_joint_position_controller lift_horizontal_joint_position_controller joint_state_controller"/>


</launch>